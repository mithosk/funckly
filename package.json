{
  "name": "funckly",
  "version": "1.4.19",
  "description": "Simple Web API Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  },
  "jest": {
    "rootDir": "dist",
    "coverageDirectory": "../coverage/",
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 91,
        "functions": 95,
        "lines": 95
      }
    }
  },
  "scripts": {
    "build": "tsc --build",
    "test": "jest --coverage",
    "conform": "prettier --write \"src\" && eslint \"src\" --fix",
    "btest": "tsc --build && jest --coverage"
  },
  "author": "Luca Nicolini <mithosk85@gmail.com>",
  "repository": "mithosk/funckly",
  "license": "Apache-2.0",
  "keywords": [
    "framework",
    "http",
    "api",
    "rest",
    "restful",
    "functional"
  ]
}
